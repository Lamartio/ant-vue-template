{
  "name": "test-ant-app-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5174",
    "build": "pnpm clear && vite build && pnpm copy-config && pnpm zip",
    "copy-config": "pnpm dlx shx cp app-config.json ./dist/app-config.json",
    "zip": "cd ./dist && pnpm dlx bestzip vue-sample.zip * ",
    "clear": "pnpm dlx shx rm -rf dist"
  },
  "dependencies": {
    "@antcde/connect-ts": "^0.0.49",
    "@antcde/ant-component-library": "^0.0.2",
    "axios": "^1.7.9",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "@mdi/font": "^7.4.47",
    "@vueuse/core": "^12.0.0",
    "vue-i18n": "^10.0.1",
    "vuetify": "^3.7.4",
    "@iconify-json/mdi": "^1.2.2",
    "@iconify/vue": "^4.3.0",
    "unplugin-icons": "^22.0.0"
  },
  "devDependencies": {
    "@intlify/unplugin-vue-i18n": "^6.0.3",
    "@vitejs/plugin-vue": "^5.2.1",
    "typescript": "^5.5.3",
    "vite": "^6.0.3",
    "vite-plugin-singlefile": "^2.1.0",
    "vue-tsc": "^2.0.29",
    "vite-plugin-vuetify": "^2.0.4"
  }
